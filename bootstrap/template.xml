<?xml version="1.0" encoding="UTF-8" standalone='yes'?>
<template>
	<version>1.0</version>
	<name>bootstrap</name>
	<name_friendly>Twitter Bootstrap</name_friendly>
	<description>This version of twitter bootstrap is packaged with the base css, javascript, and html scaffolding. Outside of a header and footer snippet, no other views are created. Based on twitter bootstrap: http://twitter.github.com/bootstrap/ </description>
	<author>Twitter Team</author>
	<link>http://twitter.github.com/bootstrap/</link>
	<image src="/media/template_image.png" />
	
	<sort>5</sort>
	
	<views>
		<!-- loader -->
		<view type="loader" name="loader" file="/views/loader.tpl" />	
		
		<!-- header -->	
		<view type="snippet" name="header" file="/views/header.tpl" />	
		
		<!-- footer -->
		<view type="snippet" name="footer" file="/views/footer.tpl" />
		
	</views>
	
	<blueprints>
		<blueprint system="bootstrap-homepage" locked="true" />
		
		<!-- basic page blueprint -->
		<blueprint system="basic-page" deactivated="" />
		
		<!-- blog blueprint -->
		<blueprint system="blog" deactivated="" />
		
		<!-- basic blog blueprint -->
		<blueprint system="blog_basic" deactivated="" />
	</blueprints>
	
	<!-- clippings -->	
	<clippings>
		<clipping name="site_name" name_friendly="Site Name" value="Edit Site Name in Content Clippings" required="0" datatype="text" sort="1" locked="1" />
		<clipping name="logo" name_friendly="Site Logo" value="0" required="0" datatype="images" sort="2" locked="1" />
		<clipping name="footer_text" name_friendly="Footer Text" value="Footer text can be updated in Content Clippings" required="0" datatype="text" sort="3" locked="0" />
		<clipping name="glyphicons" name_friendly="Glyphicons" value="/media/glyphicons-halflings.png" required="0" datatype="images" sort="99" locked="1" />
		<clipping name="glyphicons-white" name_friendly="Glyphicons White" value="/media/glyphicons-halflings-white.png" required="0" datatype="images" sort="100" locked="1" />	
	</clippings>
	

	<!-- head files -->	
	<head>
		<item nodeName="html" isLocked="1" active="1" value="html5" />
		<item type="meta" nodeName="meta" isLocked="0" active="1" sort="1">
			<http-equiv>Content-Type</http-equiv>
			<content>text/html;charset=utf-8</content>
		</item>
		<item type="meta" nodeName="meta" isLocked="0" active="1" sort="1">
			<name>viewport</name>
			<content>width=device-width, initial-scale=1.0</content>
		</item>
		
		<item type="text/css" nodeName="link" isLocked="0" active="1" file_name="bootstrap" sort="3" css_file="/resources/css/bootstrap.css" less_file="/resources/less/bootstrap.less" />
		<item type="text/css" nodeName="link" isLocked="0" active="1" file_name="reponsive" sort="5"  css_file="/resources/css/bootstrap-reponsive.css" less_file="/resources/less/bootstrap-reponsive.less" />
		<item type="text/javascript" nodeName="script" active="1" src="http://code.jquery.com/jquery-latest.min.js" />
		<item type="text/javascript" nodeName="script" isLocked="0" active="1" file_name="bootstrap.js" sort="10" file="/resources/js/bootstrap.js" />
		
	</head>
	
	<settings>
		<setting category="general" name="robots_on" label="Search Engine Crawlable?" type="checkbox"  options="0,1" value="0"  />
		<!--<setting category="general" name="site_protocol" label="HTTPS on?" type="checkbox" options="http,https" />-->
		<!--<setting category="general" name="preferred_domain_prefix" label="Always prepend WWW to my domain?" type="checkbox" options="0,1" />-->
		
		<setting category="developer" name="parsley_debugger" label="Use Parsley Debugger" type="checkbox" options="1,2" value="1"  />
		<setting category="developer" name="tips" label="Show Zesty Tips?" type="checkbox" options="0,1" value="1"   />
	</settings>

</template>

